# Primary LLM - Gemini (get from https://aistudio.google.com/app/apikey)
GOOGLE_API_KEY=your_gemini_api_key_here

# Backup LLM - Groq (get from https://console.groq.com/keys - FREE!)
GROQ_API_KEY=your_groq_api_key_here

# LLM Selection (primary, backup, or auto)
LLM_PROVIDER=auto

# Security - API Key for endpoint protection (REQUIRED for production)
API_KEY=your_secure_api_key_here

# Security - Development Mode (set to "true" to disable API auth for local dev)
# WARNING: NEVER set this to "true" in production!
DEV_MODE=false

# Business Logic - Auto-execute threshold (0.0-1.0, default 0.6)
# Orders with confidence >= threshold are auto-executed
AUTO_EXECUTE_THRESHOLD=0.6

# Application Settings
LOG_LEVEL=INFO
METRICS_PORT=9090

# Phase 2: Notifications
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL
NOTIFICATION_EMAIL=alerts@yourcompany.com

# Phase 2: Redis Cache (optional)
REDIS_URL=redis://localhost:6379

# Rate Limiting
RATE_LIMIT_PER_MINUTE=30


